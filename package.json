{
  "name": "shopify-nuxt-koa",
  "version": "1.0.0",
  "description": "Shopify app on Nuxt.js and Koa.js",
  "author": "Akito Tabira",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_ENV=development nodemon server/index.js --watch server",
    "build": "nuxt build",
    "start": "cross-env NODE_ENV=production node server/index.js",
    "generate": "nuxt generate",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "sequelize": "cd server/ && ../node_modules/sequelize-cli/lib/sequelize",
    "test": "jest",
    "docs": "docsify serve docs"
  },
  "dependencies": {
    "@koa/cors": "2",
    "@koa/router": "^8.0.8",
    "@nuxtjs/axios": "^5.3.6",
    "@nuxtjs/dotenv": "^1.4.0",
    "@shopify/koa-shopify-auth": "^3.1.57",
    "cross-env": "^5.2.0",
    "isomorphic-fetch": "^2.2.1",
    "koa": "^2.6.2",
    "koa-bodyparser": "^4.2.1",
    "koa-session": "^5.13.1",
    "nuxt": "^2.0.0",
    "pg": "^8.5.1",
    "pg-hstore": "^2.3.3",
    "sequelize": "^6.3.5",
    "sequelize-cli": "^6.2.0"
  },
  "devDependencies": {
    "@nuxtjs/eslint-config": "^2.0.0",
    "@nuxtjs/eslint-module": "^1.0.0",
    "@vue/test-utils": "^1.0.0-beta.27",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.1.0",
    "docsify-cli": "^4.4.0",
    "eslint": "^6.1.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-nuxt": ">=0.4.2",
    "eslint-plugin-prettier": "^3.1.2",
    "jest": "^24.1.0",
    "nodemon": "^1.18.9",
    "prettier": "^1.19.1",
    "vue-jest": "^4.0.0-0"
  }
}
